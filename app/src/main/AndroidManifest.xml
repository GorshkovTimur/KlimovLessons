<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.timmyg.klimovlessons">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat"
        android:usesCleartextTraffic="true">
        <activity android:name=".googlemap.GeoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".notepad.NotepadSettingsActivity" />
        <activity android:name=".notepad.NotepadActivity">

        </activity>
        <activity android:name=".preference.PreferencesActivity" />
        <activity android:name=".camera.CameraActivity" />
        <activity android:name=".listview.DetailViewActivity" />
        <activity android:name=".listview.ListViewActivity" />
        <activity android:name=".locale.LocaleActivity" />
        <activity android:name=".keyactivity.KeyActivity" />
        <activity android:name=".webview.BrowserActivity" />
        <activity android:name=".Animation.AnimationActivity" />
        <activity android:name=".GraphicActivity" />
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>